{
  "expo": {
    "name": "qsafe-mobile",
    "slug": "qsafe-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/qsafe-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "otpauth",
    "splash": {
      "image": "./assets/qsafe-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#05070d"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/qsafe-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.qsafe.app",
      "softwareKeyboardLayoutMode": "resize",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "otpauth",
              "host": "totp",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "notification": {
      "icon": "./assets/qsafe-logo.png",
      "color": "#00e5ff"
    },
    "plugins": [
      "expo-notifications"
    ],
    "web": {
      "favicon": "./assets/qsafe-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "075010ea-9d6f-428c-b4e7-380a2b13a036"
      }
    },
    "owner": "tharunik19"
  }
}
